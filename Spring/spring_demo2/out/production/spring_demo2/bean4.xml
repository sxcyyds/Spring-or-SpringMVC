<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="orders" class="com.atguigu.spring.collecttype.bean.Orders" init-method="initMethod" destroy-method="destory">
        <property name="oname" value="华为手机"></property>
    </bean>
    <!--配置后置处理器-->
    <bean id="myBeanPost" class="com.atguigu.spring.collecttype.bean.MyBeanPost"></bean>
</beans>